{
    "name": "aedart/athenaeum-http-clients",
    "description": "Http Client wrapper along with a Manager that is able to handle multiple client instances (profiles)",
    "keywords": [
        "Athenaeum",
        "Http Clients",
        "Psr-7",
        "Manager",
        "Profiles"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "guzzlehttp/guzzle": "^7.1",
        "psr/http-message": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "illuminate/pipeline": "^8.15",
        "shrikeh/teapot": "^2.3",
        "aedart/athenaeum-container": "^5.3.4",
        "aedart/athenaeum-contracts": "^5.3.4",
        "aedart/athenaeum-http-cookies": "^5.3.4",
        "aedart/athenaeum-http-messages": "^5.3.4",
        "aedart/athenaeum-support": "^5.3.4"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Http\\Clients\\": "src"
        }
    },
    "suggest": {
        "symfony/var-dumper": "Required for using debugging utilities",
        "illuminate/log": "Required for logging requests / response"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "5.4.x-dev"
        }
    }
}
