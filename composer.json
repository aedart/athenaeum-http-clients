{
    "name": "aedart/athenaeum-http-clients",
    "description": "Http Client wrapper along with a Manager that is able to handle multiple client instances (profiles)",
    "keywords": ["Athenaeum", "Http Clients", "Psr-7", "Manager", "Profiles"],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "aedart/athenaeum-container": "^6.0",
        "aedart/athenaeum-contracts": "^6.0",
        "aedart/athenaeum-http-cookies": "^6.0",
        "aedart/athenaeum-http-messages": "^6.0",
        "aedart/athenaeum-support": "^6.0",
        "guzzlehttp/guzzle": "^7.4.1",
        "illuminate/pipeline": "v9.0.1",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "shrikeh/teapot": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Http\\Clients\\": "src"
        }
    },
    "suggest": {
        "symfony/var-dumper": "Required for using debugging utilities",
        "illuminate/log": "Required for logging requests / response"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
