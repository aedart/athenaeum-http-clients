{
    "name": "aedart/athenaeum-http-clients",
    "description": "Http Client wrapper along with a Manager that is able to handle multiple client instances (profiles)",
    "keywords": [
        "Athenaeum",
        "Http Clients",
        "Psr-7",
        "Manager",
        "Profiles"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "guzzlehttp/guzzle": "~6.5",
        "aedart/athenaeum-contracts": "dev-master",
        "aedart/athenaeum-support": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Http\\Clients\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
